{
  "name": "burgerschap",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "ts-node": "ts-node",
    "start:storybook:ui": "nx run ui:storybook --host 0.0.0.0",
    "watch": "cross-env NODE_ENV=production && cross-env TAILWIND_MODE=watch",
    "console:dev": "ts-node --project ./apps/functions/tsconfig.console.json -r tsconfig-paths/register apps/functions/src/console.ts",
    "start:leerling": "npm run watch && nx serve burgerschap-leerling --port 4200",
    "start:leraar": "npm run watch && nx serve burgerschap-leraar --port 4201",
    "start:functions": "nx build functions && npm install --prefix dist/apps/functions && firebase emulators:start --inspect-functions",
    "build:leerling:prod": "nx build burgerschap-leerling --prod",
    "build:leraar:prod": "nx build burgerschap-leraar --prod",
    "build:functions": "nx build functions",
    "build:storybook:ui": "nx run ui:storybook",
    "deploy:rules": "firebase use burgerschap-dev && firebase deploy --only firestore:rules",
    "deploy:functions": "firebase use burgerschap-dev && firebase deploy --only functions",
    "deploy:storybook": "firebase use burgerschap-dev && firebase deploy --only hosting:burgerschap-storybook",
    "deploy:leerling": "firebase use burgerschap-dev && firebase deploy --only hosting:burgerschap-leerling",
    "deploy:leraar": "firebase use burgerschap-dev && firebase deploy --only hosting:burgerschap-leraar",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "firebase:emulate": "firebase emulators:start",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "delete:cache": "del-cli --force node_modules/.cache/nx"
  },
  "private": true,
  "dependencies": {
    "@headlessui/react": "1.3.0",
    "@storybook/addon-actions": "~6.3.6",
    "@storybook/addon-essentials": "~6.3.6",
    "@storybook/addon-knobs": "~6.3.0",
    "@storybook/addon-links": "~6.3.6",
    "@storybook/addon-storysource": "~6.3.6",
    "@storybook/react": "~6.3.6",
    "autoprefixer": "^10.3.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "document-register-element": "1.13.1",
    "firebase": "^8.8.0",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.14.1",
    "framer-motion": "^4.1.17",
    "postcss": "^8.3.6",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.2",
    "react-firebaseui": "5.0.2",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7",
    "rx-firebase-store": "0.5.0",
    "tailwindcss": "^2.2.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "12.6.2",
    "@nrwl/cypress": "12.6.2",
    "@nrwl/eslint-plugin-nx": "12.6.2",
    "@nrwl/jest": "12.6.2",
    "@nrwl/linter": "12.6.2",
    "@nrwl/node": "^12.6.2",
    "@nrwl/react": "12.6.2",
    "@nrwl/storybook": "^12.6.2",
    "@nrwl/tao": "12.6.2",
    "@nrwl/web": "12.6.2",
    "@nrwl/workspace": "12.6.2",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/react": "~6.3.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "11.2.6",
    "@types/exceljs": "^1.3.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.8",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "babel-jest": "27.0.6",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.3",
    "cypress": "^7.3.0",
    "depcheck": "^1.3.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "exceljs": "^4.2.1",
    "jest": "27.0.3",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5",
    "url-loader": "^3.0.0"
  }
}
